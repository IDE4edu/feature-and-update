      <import plugin="org.eclipse.jdt.junit" version="3.6.0" match="greaterOrEqual"/>
      <import plugin="EduRideFeedbackView" version="1.0.2" match="greaterOrEqual"/>
      <import plugin="org.eclipse.jface"/>
      <import plugin="org.eclipse.swt"/>
